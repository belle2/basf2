0.00012  B0long        B-            pi+       anti-B0long                 PHSP_B_MIX  dm_B0   ;
0.00012  anti-B0long   B+            pi-       B0long                      PHSP_B_MIX  dm_B0   ;

#higher excited B decays
0.00100   B_2*+        B-                                                   PHSP                   ;
0.00100   B+           B_2*-                                                PHSP                   ;
