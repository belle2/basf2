0.03595   B*+          B-                                                   PHSP                   ;
0.03595   B+           B*-                                                  PHSP                   ;

# B B  SUM=0.05772
0.02886  B+            B-                                                   VSS                    ;
0.02886  B0long        anti-B0long                                          PHSP_BB_MIX  dm_B0  -1 ;

# B* B* pi SUM=0.01084
0.00181  B0heavy       anti-B0heavy  pi0                                    PHSP_BB_MIX  dm_B0  -1 ;
