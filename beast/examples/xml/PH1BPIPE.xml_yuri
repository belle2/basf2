<?xml version="1.0" encoding="UTF-8"?>
<DetectorComponent name="PH1BPIPE">
  <Creator library="beast">PH1BPIPECreator</Creator>
  <Content>
    <!-- normally, parameters should be defined with a unit attribute. This
         enables automatic conversion to the default framework unit --> 
    <stepSize unit="um">200</stepSize>
    <!--if we don't supply a unit there can be no conversion but is still valid -->
    <divisions>32</divisions>
    <!-- valid values for false are: "", "0", "off", "false" (case
         insensitive), everything else will be interpreted as true -->
    <foo>true</foo>
    <!-- we can also have arrays as parameters which makes it easy to get a
         number of values as vector<double>.  The numbers can be divided by
         whitespace, comma or semi-colon -->

    <MaterialPipe>MaterialPipe</MaterialPipe>
    <MaterialTiN>MaterialTiN</MaterialTiN>
    <MatVacuum>Vacuum</MatVacuum>
    <x_ph1bpipe unit="cm">0.0</x_ph1bpipe>
    <y_ph1bpipe unit="cm">0.0</y_ph1bpipe>
    <z_ph1bpipe unit="cm">0.0</z_ph1bpipe>
    <!--pipe_outerRadius_x unit="cm">4.0</pipe_outerRadius_x-->
    <!--pipe_outerRadius_y unit="cm">4.4</pipe_outerRadius_y-->
    <!--pipe_innerRadius_x unit="cm">3.6</pipe_innerRadius_x-->
    <!--pipe_innerRadius_y unit="cm">4.0</pipe_innerRadius_y-->
    <!--pipe_innerRadiusTiN_x unit="cm">3.59998</pipe_innerRadiusTiN_x-->
    <!--pipe_innerRadiusTiN_y unit="cm">3.99998</pipe_innerRadiusTiN_y--> 
    <pipe_outerRadius_x unit="cm">4.4</pipe_outerRadius_x>
    <pipe_outerRadius_y unit="cm">4.4</pipe_outerRadius_y>
    <pipe_innerRadius_x unit="cm">4.0</pipe_innerRadius_x>
    <pipe_innerRadius_y unit="cm">4.0</pipe_innerRadius_y>
    <pipe_innerRadiusTiN_x unit="cm">3.99998</pipe_innerRadiusTiN_x>
    <pipe_innerRadiusTiN_y unit="cm">3.99998</pipe_innerRadiusTiN_y> 
    <pipe_hz unit="cm">20.0</pipe_hz>
    <endcap_outerRadius unit="cm">5.0</endcap_outerRadius>
    <endcap_hz unit="cm">2.2</endcap_hz>
    <xpipe_innerRadiusTiN unit="cm">3.99998</xpipe_innerRadiusTiN>
    <xpipe_innerRadius unit="cm">4.0</xpipe_innerRadius>
    <xpipe_outerRadius unit="cm">4.5</xpipe_outerRadius>
    <xpipe_hz1 unit="cm">200</xpipe_hz1>
