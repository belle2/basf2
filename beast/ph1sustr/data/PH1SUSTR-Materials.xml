<?xml version="1.0" encoding="UTF-8"?>
<!-- define our own special Materials -->
<Materials>
  <Material name="PH1SUSTR-Material">
    <density unit="g/cm3">2.5</density>
    <!-- (optional) State can be one of Gas, Liquid, Solid, Undefined, case insensitive  -->
    <state>Solid</state>
    <!-- (optional) Temperature in K, if omitted, the standard value is used (273.15) -->
    <temperature>273.15</temperature>
    <!-- (optional) Pressure in atm, if omitted, 1.0 is used -->
    <pressure>1.0</pressure>
    <!-- Components of the Material, can be any combination of Elements and Materials
         All chemical elements are predefined as Element and Material.
         If you combine only materials you don't have to specify the density above since it can be calculated
         automatically -->
    <Components>
      <Element fraction="0.75">Si</Element>
      <Element fraction="0.20">Ga</Element>
      <Material fraction="0.05">Al</Material>
    </Components>
  </Material>

  <Material name="PH1SUSTR"> <!-- Bi4Ge3O12 -->
    <density unit="g/cm3">7.130</density>
    <Components>
       <Element fraction="0.6710">Bi</Element>
       <Element fraction="0.1749">Ge</Element>
       <Element fraction="0.1541">O</Element>
    </Components>
  </Material>

   <Material name="SiO2">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.333333">Si</Material>     
      <Material fraction="0.666667">O</Material> 
    </Components>  
   </Material>

   <Material name="CaO">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.5">Ca</Material> 
      <Material fraction="0.5">O</Material>     
    </Components>  
   </Material>

   <Material name="Al2O3">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.4">Al</Material> 
      <Material fraction="0.6">O</Material>     
    </Components>  
   </Material>

   <Material name="MgO">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.5">Mg</Material> 
      <Material fraction="0.5">O</Material>     
    </Components>  
   </Material>

   <Material name="B2O3">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.4">B</Material> 
      <Material fraction="0.6">O</Material>     
    </Components>  
   </Material>

   <Material name="TiO2">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.333333">Ti</Material> 
      <Material fraction="0.666667">O</Material>     
    </Components>  
   </Material>

   <Material name="Na2O">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.666667">Na</Material> 
      <Material fraction="0.333333">O</Material>     
    </Components>  
   </Material>

   <Material name="K2O">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.666667">K</Material> 
      <Material fraction="0.333333">O</Material>     
    </Components>  
   </Material>

   <Material name="Fe2O3">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.4">Fe</Material> 
      <Material fraction="0.6">O</Material>     
    </Components>  
   </Material>

   <Material name="ZnO">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.5">Zn</Material> 
      <Material fraction="0.5">O</Material>     
    </Components>  
   </Material>

   <Material name="SO3">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.25">S</Material> 
      <Material fraction="0.75">O</Material>     
    </Components>  
   </Material>

   <Material name="FGA">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.72">SiO2</Material> 
      <Material fraction="0.1">CaO</Material>     
      <Material fraction="0.006">Al2O3</Material>    
      <Material fraction="0.025">MgO</Material>     
      <Material fraction="0.142">Na2O</Material>     
      <Material fraction="0.007">SO3</Material>     
    </Components>  
   </Material>

   <Material name="Epoxy_C15H44O7">
     <state>Solid</state>
     <density unit="g/cm3">1.3</density>
     <Components>   
      <Material fraction="0.227273">C</Material> 
      <Material fraction="0.666667">H</Material>     
      <Material fraction="0.106060">O</Material>    
    </Components>  
   </Material>

   <Material name="FG_Epoxy">
     <state>Solid</state>
     <Components>   
      <Material fraction="0.4">FGA</Material> 
      <Material fraction="0.6">Epoxy_C15H44O7</Material>     
    </Components>  
   </Material>
</Materials>
