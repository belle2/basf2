#!/bin/bash

dir="~/belle2/release/daq/slc/extra/bin"
for i in $(seq 5001 5013)
do
echo "booting cprcontrold in cpr${i}"
ssh cpr${i} "${dir}/boot_daemon cprcontrold cpr${i}"
usleep 100000
done

#cd $dir
echo "booting rocontrold in ecl01"
./boot_daemon rocontrold ropc_ecl01
sleep 2
echo "booting ecl01 runcontrold in ecl01"
./boot_daemon runcontrold ecl01
sleep 2
echo "booting ecl runcontrold in ecl01"
./boot_daemon runcontrold ecl
