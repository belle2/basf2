#!/bin/bash

label=`date +%Y-%m-%d-%H-%M`

startBySSH() {
  ssh $1 "mkdir -p ~/run/$2/$1"
  ssh $1 "source ~/belle2/release/daq/slc/extra/bin/setup.sh ~/belle2 release local $2 &> /dev/null" &
}

startBySSH belle-rpc1 nsmd2
sleep 2
startBySSH cpr010 nsmd2
sleep 2
startBySSH cpr011 nsmd2
sleep 2
startBySSH ttd10 nsmd2
