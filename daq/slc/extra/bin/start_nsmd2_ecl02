#!/bin/bash

cd $HOME/belle2/release
source ../tools/setup_belle2
setuprel
cd -

echo "booting nsmd2"
for i in $(seq 5014 5018)
do
./boot_nsmd2_ssh cpr${i} tkonno local
done

for i in $(seq 6001 6008)
do
./boot_nsmd2_ssh cpr${i} tkonno local
done

./boot_nsmd2_ssh ecl02 tkonno local
./boot_nsmd2_ssh ecl02 tkonno global
