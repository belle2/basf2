#!/bin/bash

label=`date +%Y-%m-%d-%H-%M`

startBySSH() {
  ssh $1 "mkdir -p ~/run/$4"
  ssh $1 "source ~/belle2/release/daq/slc/extra/bin/setup.sh $2 $3 global $4 &> /dev/null"
}

ssh belle-rpc2 "source ~/belle2/release/daq/slc/extra/bin/setup.sh ~/belle2 release global nsmd2 &> /dev/null"
#sleep 2
ssh belle-reco "source ~/hlt/belle2/latest/daq/slc/extra/bin/setup.sh ~/hlt/belle2 latest global nsmd2 &> /dev/null"

