#!/bin/bash

label=`date +%Y-%m-%d-%H-%M`

kill `ps aux | grep "runcontrold svd" | grep -v grep | awk '{print $2}'`
sleep 1
kill `ps aux | grep "runcontrold ttd" | grep -v grep | awk '{print $2}'`
sleep 1
kill `ps aux | grep "runcontrold runcontrol" | grep -v grep | awk '{print $2}'`
sleep 1
source ~/belle2/release/daq/slc/extra/bin/nsm2_global.sh
sleep 2
runcontrold svd
#ssh belle-rpc1 "source ~/belle2/release/daq/slc/extra/bin/setup.sh ~/belle2 release global runcontrold svd" &
#sleep 1
runcontrold ttd
#ssh belle-rpc1 "source ~/belle2/release/daq/slc/extra/bin/setup.sh ~/belle2 release global runcontrold ttd" &
sleep 1
runcontrold runcontrol
#ssh belle-rpc1 "source ~/belle2/release/daq/slc/extra/bin/setup.sh ~/belle2 release global runcontrold runcontrol" &

