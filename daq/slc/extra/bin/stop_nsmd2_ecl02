#!/bin/bash

cd $HOME/belle2/release
source ../tools/setup_belle2
setuprel
cd -

echo "killing nsmd2"
for i in $(seq 5014 5018)
do
ssh tkonno@cpr${i} "killall basf2 > /dev/null"
ssh tkonno@cpr${i} "killall nsmd2 datad > /dev/null"
done
for i in $(seq 6001 6008)
do
ssh tkonno@cpr${i} "killall basf2 > /dev/null"
ssh tkonno@cpr${i} "killall nsmd2 datad > /dev/null"
done
killall nsmd2 datad > /dev/null
