record(stringout, "B2_nsm:set:$(nodename):rcrequest")
{
	field(DESC, "Sent request to NSM2 node")
	field(SCAN, "Passive")
	field(VAL, "UNKNOWN")
	field(DTYP, "nsm2_vset_stringout")
}

record(stringin, "B2_nsm:get:$(nodename):rcstate")
{
	field(DESC, "RC state response to NSM2 request") 
	field(SCAN, "I/O Intr")
	field(VAL,  "NOTREADY") 
	field(DTYP, "nsm2_vget_stringin")
}

record(stringout, "B2_nsm:set:$(nodename):rcrequest")
{
	field(DESC, "Sent request to NSM2 node")
	field(SCAN, "Passive")
	field(VAL, "UNKNOWN")
	field(DTYP, "nsm2_vset_stringout")
}

