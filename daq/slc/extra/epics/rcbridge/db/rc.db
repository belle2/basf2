record(longin, "$(user):rc:expno")
{
	field(DESC, "Received Exp number from NSM2")
	field(SCAN, "I/O Intr")
	field(VAL, "0")
	field(DTYP, "rc_bridge_expno")
}

record(longin, "$(user):rc:runno")
{
	field(DESC, "Received Run number from NSM2")
	field(SCAN, "I/O Intr")
	field(VAL, "0")
	field(DTYP, "rc_bridge_runno")
}

record(longin, "$(user):rc:subno")
{
	field(DESC, "Received Sub number from NSM2")
	field(SCAN, "I/O Intr")
	field(VAL, "0")
	field(DTYP, "rc_bridge_subno")
}

record(longin, "$(user):rc:configid")
{
	field(DESC, "Received Confid ID from NSM2")
	field(SCAN, "I/O Intr")
	field(VAL, "0")
	field(DTYP, "rc_bridge_configid")
}

record(stringin, "$(user):rc:request")
{
	field(DESC, "Received RC request from NSM2")
	field(SCAN, "I/O Intr")
	field(VAL, "STATECHEK")
	field(DTYP, "rc_bridge_request")
}

record(stringout, "$(user):rc:state")
{
	field(DESC, "RC state response to NSM2") 
	field(SCAN, "Passive")
	field(VAL,  "NOTREADY") 
	field(DTYP, "rc_bridge_state")
}
