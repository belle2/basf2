<VBox fx:id="pane" prefHeight="549.0" prefWidth="803.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="b2daq.apps.monitor.DataFlowMonitorController">
    <children>
        <HBox prefHeight="84.0" prefWidth="907.0">
            <children>
                <VBox maxWidth="343.0" prefHeight="195.0" prefWidth="343.0" HBox.hgrow="NEVER">
                    <children>
                        <HBox alignment="CENTER_LEFT" minHeight="35.0" prefHeight="49.0" prefWidth="292.0" VBox.vgrow="NEVER">
                            <children>
                                <Label text="R/O State:">
                                    <HBox.margin>
                                        <Insets right="10.0" />
                                    </HBox.margin>
                                </Label>
                                <RCStateLabel fx:id="state" HBox.hgrow="ALWAYS" />
                            </children>
                        </HBox>
                        <GridPane hgap="10.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="93.0" minWidth="10.0" prefWidth="54.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.0" minWidth="0.0" prefWidth="30.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="189.0" minWidth="10.0" prefWidth="76.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="169.0" minWidth="10.0" prefWidth="27.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="135.0" minWidth="10.0" prefWidth="69.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label fx:id="label_src" text="Sender" />
                                <Label fx:id="label_dest" text="Receiver" GridPane.columnIndex="4" />
                                <CanvasPanel minHeight="10.0" minWidth="20.0" prefHeight="30.0" prefWidth="20.0" GridPane.columnIndex="1">
                                    <canvas>
                                        <HistogramCanvas fx:id="c_in" fillColor="transparent" lineColor="transparent" name="c_node0_cur_gr" stat="false" title="">
                                            <shapes>
                                                <FXCollections fx:factory="observableArrayList">
                                                    <GArrow headLength="0.3" headWidth="0.1" length="0.6" lineColor="red" lineWidth="2" width="0.4" x="0.05" y="0.3" />
                                                    <GLine lineColor="red" lineWidth="2" x1="0.1" x2="0.9" y1="0.1" y2="0.9" />
                                                    <GLine lineColor="red" lineWidth="2" x1="0.9" x2="0.1" y1="0.1" y2="0.9" />
                                                </FXCollections>
                                            </shapes>
                                        </HistogramCanvas>
                                    </canvas>
                                </CanvasPanel>
                                <Label fx:id="label_nodename" minHeight="45.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="45.0" prefWidth="82.0" text="NODENAME" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.hgrow="SOMETIMES">
                                    <font>
                                        <Font name="System Bold" size="13.0" />
                                    </font>
                                </Label>
                                <CanvasPanel minHeight="10.0" minWidth="10.0" prefHeight="30.0" prefWidth="47.0" GridPane.columnIndex="3">
                                    <canvas>
                                        <HistogramCanvas fx:id="c_out" fillColor="transparent" lineColor="transparent" name="c_node0_cur_gr" stat="false" title="">
                                            <shapes>
                                                <FXCollections fx:factory="observableArrayList">
                                                    <GArrow headLength="0.3" headWidth="0.1" length="0.6" lineColor="red" lineWidth="2" width="0.4" x="0.05" y="0.3" />
                                                    <GLine lineColor="red" lineWidth="2" x1="0.1" x2="0.9" y1="0.1" y2="0.9" />
                                                    <GLine lineColor="red" lineWidth="2" x1="0.9" x2="0.1" y1="0.1" y2="0.9" />
                                                </FXCollections>
                                            </shapes>
                                        </HistogramCanvas>
                                    </canvas>
                                </CanvasPanel>
                            </children>
                        </GridPane>
                        <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="35.0" prefWidth="863.0">
                            <children>
                                <Label text="Run #:">
                                    <HBox.margin>
                                        <Insets right="10.0" />
                                    </HBox.margin>
                                </Label>
                                <Label fx:id="label_runno" text="0000.0000.000" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="35.0" prefWidth="863.0">
                            <children>
                                <Label text="Update time:">
                                    <HBox.margin>
                                        <Insets right="10.0" />
                                    </HBox.margin>
                                </Label>
                                <Label fx:id="label_ctime" text="--:--:-- ----/--/--" />
                            </children>
                        </HBox>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </VBox>
                <TableView fx:id="table_stat" prefHeight="215.0" prefWidth="523.0">
                    <columns>
                        <TableColumn fx:id="col_io" prefWidth="40.0" text="I/O">
                            <cellValueFactory>
                                <PropertyValueFactory property="iotype" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="col_count" text="Count">
                            <cellValueFactory>
                                <PropertyValueFactory property="count" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="col_rate" prefWidth="129.0" text="Count rate [Hz]">
                            <cellValueFactory>
                                <PropertyValueFactory property="freq" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="col_size" prefWidth="126.0" text="Event size [kB]">
                            <cellValueFactory>
                                <PropertyValueFactory property="size" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="col_size1" prefWidth="140.0" text="Event rate [kB/s]">
                            <cellValueFactory>
                                <PropertyValueFactory property="rate" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </TableView>
            </children>
        </HBox>
        <VBox HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
            <children>
                <CanvasPanel maxHeight="1.7976931348623157E308" prefHeight="180.0" prefWidth="593.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
                    <canvas>
                        <HistogramCanvas fx:id="c_size" fillColor="lightgray" fontSize="0.9" name="c_node0_voltage_time" stat="false" title=" " yLabelAlignment="bottom">
                            <pad>
                                <Pad height="0.70" x="0.16" y="0.3" />
                            </pad>
                            <histograms>
                                <FXCollections fx:factory="observableArrayList">
                                    <TimedGraph1D fx:id="gr_size_in" fixYmax="true" fixYmin="true" lineColor="blue" name="gr_size_in" nbinsx="2000" title="Output;Time;" xmax="3600" xmin="0" ymax="5" ymin="0" />
                                    <TimedGraph1D fx:id="gr_size_out" lineColor="red" name="gr_size_out" nbinsx="1000" title="Input;Time;Event size [Byte/event]" xmax="3600" xmin="0" />
                                </FXCollections>
                            </histograms>
                            <shapes>
                                <FXCollections fx:factory="observableArrayList">
                                    <GRect fillColor="white" height="0.2" lineColor="black" lineWidth="0.6" width="0.5" x="0.25" y="0.05" />
                                    <GLine lineColor="blue" lineWidth="2" x1="0.27" x2="0.33" y1="0.15" y2="0.15" />
                                    <GLine lineColor="red" lineWidth="2" x1="0.52" x2="0.58" y1="0.15" y2="0.15" />
                                    <GText alignment="midle left" fontSize="0.6" text="Input" x="0.35" y="0.19" />
                                    <GText alignment="midle left" fontSize="0.6" text="Output" x="0.60" y="0.19" />
                                    <GText alignment="midle center" angle="270" fontSize="0.7" text="Event size" x="0.04" y="0.6" />
                                    <GText alignment="midle center" angle="270" fontSize="0.7" text="[kB/event]" x="0.07" y="0.6" />
                                </FXCollections>
                            </shapes>
                        </HistogramCanvas>
                    </canvas>
                </CanvasPanel>
                <CanvasPanel maxHeight="1.7976931348623157E308" prefHeight="180.0" prefWidth="907.0" VBox.vgrow="ALWAYS">
                    <canvas>
                        <HistogramCanvas fx:id="c_rate" fillColor="lightgray" fontSize="0.9" name="c_node0_voltage_time" stat="false" title=" " yLabelAlignment="top">
                            <pad>
                                <Pad height="0.70" x="0.16" y="0." />
                            </pad>
                            <histograms>
                                <FXCollections fx:factory="observableArrayList">
                                    <TimedGraph1D fx:id="gr_rate_in" fixYmax="true" fixYmin="true" lineColor="blue" name="gr_rate_in" nbinsx="2000" title="Input;Time;" xmax="3600" xmin="0" ymax="23" ymin="0" />
                                    <TimedGraph1D fx:id="gr_rate_out" lineColor="red" name="gr_rate_in" nbinsx="1000" title="Output;Time;" xmax="3600" xmin="0" />
                                </FXCollections>
                            </histograms>
                            <shapes>
                                <FXCollections fx:factory="observableArrayList">
                                    <GText alignment="midle center" angle="270" fontSize="0.7" text="Event rate" x="0.04" y="0.4" />
                                    <GText alignment="midle center" angle="270" fontSize="0.7" text="[kHz]" x="0.07" y="0.4" />
                                </FXCollections>
                            </shapes>
                        </HistogramCanvas>
                    </canvas>
                </CanvasPanel>
            </children>
        </VBox>
    </children>
</VBox>
