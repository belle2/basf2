<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.web.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import b2daq.apps.*?>
<?import javafx.scene.control.cell.*?>
<?scenebuilder-classpath-element ../classes?>

<TabPane prefHeight="591.0" prefWidth="944.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="b2daq.apps.pscontrol.PowerSupplyMainPaneController">
    <tabs>
        <Tab closable="false" text="Control main">
            <content>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="528.0" prefWidth="825.0">
                    <children>
                        <VBox alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefHeight="558.0" prefWidth="190.0" HBox.hgrow="ALWAYS">
                            <children>
                                <TitledPane animated="false" collapsible="false" prefHeight="102.0" prefWidth="245.0" text="Operation mode">
                                    <content>
                                        <GridPane hgap="10.0" layoutX="22.0" layoutY="6.0" prefHeight="69.0" prefWidth="243.0" vgap="10.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                            </rowConstraints>
                                            <children>
                                                <RadioButton fx:id="radio_programatic" mnemonicParsing="false" selected="true" text="Programatic">
                                                    <toggleGroup>
                                                        <ToggleGroup fx:id="group_mode" />
                                                    </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="radio_manual" mnemonicParsing="false" text="Manual" toggleGroup="$group_mode" GridPane.rowIndex="1" />
                                            </children>
                                        </GridPane>
                                    </content>
                                </TitledPane>
                                <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="251.0" prefWidth="245.0" text="Power supply editor">
                                    <content>
                                        <fx:include fx:id="powerSettings" source="PowerSettingPane.fxml" />
                                    </content>
                                </TitledPane>
                                <fx:include fx:id="commandButton" source="PowerControlCommandButtonPane.fxml" />
                            </children>
                        </VBox>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="454.0" prefWidth="354.0" HBox.hgrow="ALWAYS">
                            <children>
                                <TitledPane collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="415.0" prefWidth="581.0" text="Power supply status" VBox.vgrow="ALWAYS">
                                    <content>
                                        <fx:include fx:id="psStatus" source="PowerSupplyStatusPane.fxml" />
                                    </content>
                                </TitledPane>
                                <fx:include fx:id="logview" source="LogViewPane.fxml" />
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </content>
        </Tab>
        <Tab closable="false" text="Monitor">
            <content>
                <fx:include fx:id="hvmonitor" source="HVStatusMonitor.fxml" />
            </content>
        </Tab>
        <Tab closable="false" text="Editor">
            <content>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <fx:include fx:id="hvparameditor" source="HVParamEditorPane.fxml" />
                    </children>
                </VBox>
            </content>
        </Tab>
        <Tab closable="false" text="Network ">
            <content>
                <fx:include fx:id="networkconfig" source="NetworkConfigPane.fxml" />
            </content>
        </Tab>
    </tabs>
</TabPane>
