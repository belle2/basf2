<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox fx:id="vbox" prefHeight="505.0" prefWidth="878.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="b2daq.apps.runcontrol.CopperEditorController">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="52.0" prefWidth="796.0" spacing="10.0">
            <children>
                <Label text="Select configuration" />
                <ComboBox fx:id="combo" prefWidth="150.0" promptText="---" />
                <Button fx:id="button_reload" mnemonicParsing="false" text="Reload">
                    <font>
                        <Font size="12.0" />
                    </font>
                </Button>
                <Button fx:id="create_button" mnemonicParsing="false" text="Create config">
                    <HBox.margin>
                        <Insets left="30.0" />
                    </HBox.margin>
                </Button>
                <Label fx:id="label_message">
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </Label>
                <HBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT" spacing="10.0">
                    <children>
                        <Button fx:id="button_add_channel" mnemonicParsing="false" text="Add ROPC">
                            <HBox.margin>
                                <Insets left="10.0" right="5.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="button_multiedit" mnemonicParsing="false" text="Add Src host">
                            <HBox.margin>
                                <Insets right="5.0" />
                            </HBox.margin>
                        </Button>
                    </children>
               <HBox.margin>
                  <Insets left="80.0" />
               </HBox.margin>
                </HBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
            </padding>
        </HBox>
        <TabPane prefHeight="365.0" prefWidth="616.0" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab closable="false" text="Config">
                    <content>
                        <TableView fx:id="table_config" layoutX="21.0" layoutY="-10.0" prefHeight="365.0" prefWidth="546.0" tableMenuButtonVisible="true">
                            <columns>
                                <TableColumn fx:id="col_config_node" prefWidth="76.0" text="Node">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="node" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="col_config_copperid" prefWidth="100.0" text="ROPC ID">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="copperID" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="col_config_hslba" prefWidth="110.0" text="Host">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="hslbA" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="col_config_hslbb" prefWidth="60.0" text="Port">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="hslbB" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn fx:id="col_config_script" prefWidth="110.0" text="Basf2 R/O">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="basf2script" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn prefWidth="343.0" text="Event builder 0">
                                    <columns>
                                        <TableColumn fx:id="col_config_eb0_host" prefWidth="100.0" text="Host">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="eb0Host" />
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn fx:id="col_config_eb0_port" prefWidth="60.0" text="Port">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="eb0Port" />
                                            </cellValueFactory>
                                        </TableColumn>
                                    </columns>
                                </TableColumn>
                        <TableColumn prefWidth="153.0" text="Event builder1 TX">
                           <columns>
                              <TableColumn fx:id="col_config_eb0_host1" prefWidth="100.0" text="Host">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="eb0Host" />
                                 </cellValueFactory>
                              </TableColumn>
                              <TableColumn fx:id="col_config_eb0_port1" prefWidth="60.0" text="Port">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="eb0Port" />
                                 </cellValueFactory>
                              </TableColumn>
                           </columns>
                        </TableColumn>
                            </columns>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="8.0" />
                            </padding>
                        </TableView>
                    </content>
                </Tab>
                <Tab closable="false" text="Hosts">
                    <content>
                          <TableView fx:id="table_setup" layoutX="21.0" layoutY="-10.0" prefWidth="1200.0" tableMenuButtonVisible="true">
                              <columns>
                                  <TableColumn fx:id="col_setup_node" prefWidth="102.0" text="Node">
                                      <cellValueFactory>
                                          <PropertyValueFactory property="node" />
                                      </cellValueFactory>
                                  </TableColumn>
                                  <TableColumn prefWidth="102.0" text="Src node">
                                      <columns>
                              <TableColumn fx:id="col_setup_ttrxfirm1" prefWidth="116.0" text="Node">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="ttrxFirmware" />
                                 </cellValueFactory>
                              </TableColumn>
                                          <TableColumn fx:id="col_setup_ttrx" prefWidth="116.0" text="Host">
                                              <cellValueFactory>
                                                  <PropertyValueFactory property="ttrx" />
                                              </cellValueFactory>
                                          </TableColumn>
                                          <TableColumn fx:id="col_setup_ttrxfirm" prefWidth="116.0" text="Port">
                                              <cellValueFactory>
                                                  <PropertyValueFactory property="ttrxFirmware" />
                                              </cellValueFactory>
                                          </TableColumn>
                                      </columns>
                                  </TableColumn>
                              </columns>
                              <padding>
                                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                              </padding>
                          </TableView>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</VBox>
