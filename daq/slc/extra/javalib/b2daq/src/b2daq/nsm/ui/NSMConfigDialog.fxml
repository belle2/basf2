         <columns>
            <TableColumn fx:id="col_nsm_dataname" prefWidth="138.0" text="Data name">
               <cellValueFactory>
                  <PropertyValueFactory property="dataname" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="col_nsm_format" prefWidth="123.0" text="Format">
               <cellValueFactory>
                  <PropertyValueFactory property="format" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="col_nsm_revision" prefWidth="79.0" text="Revision">
               <cellValueFactory>
                  <PropertyValueFactory property="revision" />
               </cellValueFactory>
            </TableColumn>
