            <Label text="NSM communication setup" GridPane.columnSpan="2">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label text="Target NSM node" GridPane.rowIndex="1" />
            <TextField fx:id="field_nsm_target" maxWidth="140.0" prefHeight="25.0" prefWidth="140.0" promptText="NSM client node" text="ARICH_HV" GridPane.columnIndex="1" GridPane.rowIndex="1" />
