<?xml version="1.0" encoding="UTF-8"?>
<Materials>
  <Material name="SVD-KaptonClean">
    <state>Solid</state>
    <density unit="g/cm3">1.4</density>
    <Components>
      <Element fraction="0.2564">H</Element>
      <Element fraction="0.5641">C</Element>
      <Element fraction="0.0513">O</Element>
      <Element fraction="0.1282">N</Element>
    </Components>
  </Material>
  <Material name="SVD-Keratherm">
    <state>Solid</state>
    <density unit="g/cm3">1.9</density>
    <Components>
      <Element fraction="0.30">H</Element>
      <Element fraction="0.20">Si</Element>
      <Element fraction="0.30">O</Element>
      <Element fraction="0.20">N</Element>
    </Components>
  </Material>

 <!-- The real density should be 1.32 g/cm3. Here a larger value is used, because of the 
      limited space underneath the cooling pipe (the clamps are supposed to surround the 
      cooling pipe, but this would cause overlaps) -->
 <Material name="SVD-Peek">
    <state>Solid</state>
    <density unit="g/cm3">3.0</density>
    <Components>
      <Element fraction="0.08">H</Element>
      <Element fraction="0.76">C</Element>
      <Element fraction="0.16">O</Element>
    </Components>
  </Material>

  <Material name="SVD-Glue">
    <state>Solid</state>
    <Components>
      <Material fraction="0.4390">C</Material>
      <Material fraction="0.0732">O</Material>
      <Material fraction="0.4878">H</Material>
    </Components>
  </Material>
  <Material name="SVD-Airex">
    <density unit="g/cm3">0.0750</density>
    <state>Solid</state>
    <Components>
      <Element fraction="0.3636">C</Element>
      <Element fraction="0.0909">O</Element>
      <Element fraction="0.5000">H</Element>
      <Element fraction="0.0455">N</Element>
    </Components>
  </Material>
  <Material name="SVD-KaptonHybrid">
    <state>Solid</state>
    <Components>
      <Material fraction="0.8998">SVD-KaptonClean</Material>
      <Material fraction="0.0900">Cu</Material>
      <Material fraction="0.0078">Ni</Material>
      <Material fraction="0.0024">Au</Material>
      <!--<Material fraction="0">SVD-Glue</Material>-->
    </Components>
  </Material>
  <Material name="SVD-KaptonPitchAdapter">
    <state>Solid</state>
    <Components>
      <Material fraction="0.7887">SVD-KaptonClean</Material>
      <Material fraction="0.1577">Cu</Material>
      <Material fraction="0.0410">Ni</Material>
      <Material fraction="0.0126">Au</Material>
      <!--<Material fraction="0.0338">SVD-Glue</Material>-->
    </Components>
  </Material>

  <Material name="SVD-Carbon">
    <state>Solid</state>
    <density unit="g/cm3">1.5</density>
    <Components>
      <Element>C</Element>
    </Components>
  </Material>
</Materials>
