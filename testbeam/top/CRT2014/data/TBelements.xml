      <Width desc="Width of buttress bars" unit="mm">200.0</Width>
      <Length desc="Length of buttress bars" unit="mm">100.0</Length>
      <BoxHeight desc="Height of buttress bar box (for subtraction solid)" unit="mm">2100.0</BoxHeight>
      <BoxWidth desc="Width of buttress bars box (for subtraction solid)" unit="mm">1600.0</BoxWidth>
      <BoxLength desc="Length of buttress bars box (for subtraction solid)" unit="mm">200.0</BoxLength>
