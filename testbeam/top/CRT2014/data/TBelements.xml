      <Trap_dx desc="dx" unit="mm">13.0</Trap_dx>
      <Trap_dy1 desc="dy1" unit="mm">700</Trap_dy1>
      <Trap_dy2 desc="dy2" unit="mm">100.0</Trap_dy2>         
      <Trap_dz desc="dz" unit="mm">1200.0</Trap_dz> 
      <Box_dx desc="dx" unit="mm">800</Box_dx>
      <Box_dy desc="dy1" unit="mm">30</Box_dy>
      <Box_dz desc="dy2" unit="mm">1200.0</Box_dz> 
      <SpacingZ desc="Spacing between angleplates in Z" unit="mm">1100.0</SpacingZ>

    </AnglePlate>

    <ButtressGusset>
      <Trap_dx1 desc="dx" unit="mm">600.0</Trap_dx1>
      <Trap_dx2 desc="dy1" unit="mm">100.0</Trap_dx2>
      <Trap_dy desc="dy2" unit="mm">865.0</Trap_dy>         
