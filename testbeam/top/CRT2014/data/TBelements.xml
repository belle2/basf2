    <BlockLength desc="Length of range stack blocks" unit="mm">1000.0</BlockLength>
    <TubeRadius  desc="Radius of movement holes in blocks" unit="mm">16.0</TubeRadius>
    <TubeDepth   desc="Depth of movement holes in blocks"  unit="mm">75.0</TubeDepth>
    <TubeOffset  desc="Offset of holes from centre along long side of blocks"  unit="mm">230</TubeOffset>
  </CRTRangeStack>  

