      <EffectiveThickness desc="Effective thickness of short bars (Mizumi extrusion)" unit="mm">20.0</EffectiveThickness>
    </ButtressBar>
    <EndPlate>
      <Height desc="Height of end plates" unit="mm">600.0</Height>
      <Width desc="Width of end plates" unit="mm">500.0</Width>
      <Length desc="Length of end plates" unit="mm">13.0</Length>
