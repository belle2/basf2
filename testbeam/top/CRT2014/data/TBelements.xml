<?xml version="1.0" encoding="UTF-8"?>

<!-- definitions of basic building elements of TB geometry -->

<elements desc="counters, absorbers, supports etc.">

  <TriggerS desc="small trigger scintillator">
    <creatorName>scintillatorCounter</creatorName>
    <Material>PMMA</Material>
    <width unit="mm">5.0</width>
    <height unit="mm">5.0</height>
    <thickness unit="mm">5.0</thickness>
  </TriggerS>

  <TriggerM desc="large trigger scintillator">
    <creatorName>scintillatorCounter</creatorName>
    <Material>PMMA</Material>
    <width unit="mm">50.0</width>
    <height unit="mm">50.0</height>
    <thickness unit="mm">5.0</thickness>
  </TriggerM>


  <TriggerL desc="large trigger scintillator">
    <creatorName>scintillatorCounter</creatorName>
    <Material>PMMA</Material>
    <width unit="mm">250.0</width>
    <height unit="mm">10.0</height>
    <thickness unit="mm">250.0</thickness>
  </TriggerL>



  <SciFi desc="scintillating fiber tracker">
    <creatorName>SciFiTracker</creatorName>
    <Material desc="empty space material">Air</Material>
    <fullThickness desc="optional" unit="mm">0</fullThickness>
    <fibers>
      <Material>Polystyrene</Material>
      <coatingMaterial>PMMA</coatingMaterial>
      <fiberDiameter unit="mm">2</fiberDiameter>
      <activeDiameter unit="mm">1.94</activeDiameter>
      <fiberLength unit="mm">250</fiberLength>
      <numberOfFibers>10</numberOfFibers>
      <firstFiber desc="z- or z+, default=z-">z-</firstFiber>
      <row2rowDistance desc="optional" unit="mm">0</row2rowDistance>
    </fibers>
    <coverPlate>
      <Material>Polyacetal</Material>
      <width unit="mm">10</width>
      <height unit="mm">250</height>
      <thickness unit="mm">5.0</thickness>
    </coverPlate>
  </SciFi>

  <VetoCounter desc="veto counter">
    <creatorName>vetoCounter</creatorName>
    <Material>PMMA</Material>
    <ShieldingMaterial>ExpandedPolystyrene</ShieldingMaterial>
    <ESMaterial desc="empty space material">Air</ESMaterial>
    <width unit="mm">200.0</width>
    <height unit="mm">400.0</height>
    <thickness unit="mm">20.0</thickness>    
    <shieldingThickness unit="mm">18.0</shieldingThickness>
  </VetoCounter>


