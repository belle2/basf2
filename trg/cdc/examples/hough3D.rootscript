  float houghMesh4[nCotSteps][nZ0Steps];

  float minDiffHough = 0;

  int tempHoughZ0;
  int tempRealHoughZ0;
  double houghMaxZ0;
  // Finder
  double houghMax;
  double maxCot;
  double maxZ0;
