  TH2F* hsz00 = new TH2F("histsz0","",1000,0.2,1.5,1000,-1.5,1.5);
  hsz00->SetMarkerStyle(20);
  hsz00->SetMarkerSize(.6);

  const int nEvents = inTree->GetEntries();
